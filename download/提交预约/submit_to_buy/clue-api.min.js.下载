(function(w,d){function checkCookie(){var cookieEnabled=navigator.cookieEnabled;if(!cookieEnabled){document.cookie="testcookie";cookieEnabled=document.cookie.indexOf("testcookie")!=-1}return cookieEnabled}function init(client){var client=client;var url=location.href;var ds=d.body.clientWidth+'x'+d.body.clientHeight;var ln=navigator.language||navigator.browserLanguage;var si=(new Date()).getTime();var protocol=(('https:'===document.location.protocol)?'https://':'http://');var url=protocol+'isite.baidu.com/lmp/getbid?client='+client+'&url='+url+'&ds='+ds+'&ln='+ln+'&si='+Math.floor((+new Date)/(864e5))+'&callback=getClueInfo&cookie='+checkCookie();var script=d.createElement('script');script.setAttribute('src',url);d.getElementsByTagName('body')[0].appendChild(script)}w.clueInit=function(options){init(options.client);w.getClueInfo=options.callback}})(window,document);